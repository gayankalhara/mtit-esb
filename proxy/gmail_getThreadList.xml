<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse" name="gmail_getThreadList" transports="https,http" statistics="disable" trace="disable" startOnLoad="true">
	<target>
		<inSequence>
			<property name="userid" expression="json-eval($.userid)"/>
			<property name="accessToken" expression="json-eval($.accessToken)"/>
			<gmail.init>
				<accessToken>{$ctx:accessToken}</accessToken>
			</gmail.init>
			<gmail.getThreadList>
				<userid>{$ctx:userid}</userid>
			</gmail.getThreadList>
			<respond/>
		</inSequence>
		<outSequence>
			<log/>
			<send/>
		</outSequence>
	</target>
	<description/>
</proxy>
